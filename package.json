{
  "name": "mldocs",
  "description": "MLDocs - Documentation site for Machine Learning",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "prepare": "husky install",
    "validate": "prettier --check . && eslint .",
    "validate:fix": "prettier --write . && eslint --fix ."
  },
  "dependencies": {
    "fumadocs-core": "15.6.9",
    "fumadocs-mdx": "11.7.3",
    "fumadocs-ui": "15.6.9",
    "katex": "0.16.22",
    "lucide-react": "0.544.0",
    "next": "15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rehype-katex": "7.0.1",
    "remark-math": "6.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/mdx": "2.0.13",
    "@types/node": "24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^8",
    "eslint-config-next": "15.4.5",
    "husky": "8.0.3",
    "lint-staged": "16.1.6",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
